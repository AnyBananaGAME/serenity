{
  "name": "launcher",
  "version": "2.1.0",
  "repository": "https://github.com/SerenityJS/serenity",
  "private": true,
  "scripts": {
    "start": "node dist/index.js",
    "dev": "tsup --watch --onSuccess \"node dist/index.js\"",
    "watch": "chokidar '../packages/**/dist/*' '../packages/**/target/*.json' './src/*' --initial -c 'tsup --onSuccess \"node ./dist/index.js\"'",
    "build": "tsup",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/",
    "test": "jest --detectOpenHandles"
  },
  "jest": {
    "preset": "@serenityjs/jest-presets/jest/node"
  },
  "devDependencies": {
    "@serenityjs/chokidar": "*",
    "@serenityjs/eslint-config": "*",
    "@serenityjs/jest-presets": "*",
    "@serenityjs/typescript-config": "*",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.24",
    "jest": "^29.7.0",
    "tsup": "^8.0.2",
    "typescript": "^5.4.2"
  },
  "dependencies": {
    "@serenityjs/bedrock-data": "^0.0.9",
    "@serenityjs/binaryutils": "*",
    "@serenityjs/emitter": "*",
    "@serenityjs/nbt": "*",
    "@serenityjs/protocol": "*",
    "@serenityjs/raknet": "*",
    "chalk": "4.0.0",
    "fast-jwt": "^4.0.0",
    "moment": "^2.30.1",
    "simplex-noise": "^4.0.1"
  }
}
